{
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "faker": "random.uuid"
    },
    "name": {
      "type": "string"
    },
    "partsoSpeech": {
      "type": "array",
      "minItems": 1,
      "uniqueItems": true,
      "items": {
        "type": "string"
      },
      "faker.poS": "randomArray"
    },
    "emot": {
      "type": "object",
      "properties": {
        "confidence": {
          "type": "integer"
        }
      }
    },
    "alts": {
      "type": "array",
      "uniqueItems": true,
      "items": {
        "type": "object",
        "properties": {
          "word": {
            "type": "string"
          },
          "wordGroup": {
            "type": "string"
          },
          "confidence": {
            "type": "integer"
          },
          "viability": {
            "type": "integer"
          }
        }
      }
    }
  },
  "required": ["name", "id"]
}
