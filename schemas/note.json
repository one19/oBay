      "type": "string",
      "faker": "random.uuid"
    },
    "name": {
      "type": "string"
    },
    "content": {
      "type": "string"
    },
    "timeSpent": {
      "type": "string"
    },
    "updatedAt": {
      "type": "string"
    },
    "createdAt": {
      "type": "string"
    },
    "length": {
      "type": "integer"
    },
    "emot": {
      "type": "string"
    },
    "confidence": {
      "type": "integer"
    },
    "score": {
      "type": "integer"
    },
