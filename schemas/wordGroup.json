{
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "faker": "random.uuid"
    },
    "name": {
      "type": "string"
    },
    "words": {
      "type": "array",
      "minItems": 2,
      "maxItems": 5,
      "items": {
        "type": "object",
        "properties": {
          "word": {
            "type": "string",
            "faker": "random.uuid"
          },
          "poSpeech": {
            "type": "array",
            "faker.poS": "randomArray"
          },
          "frequency": {
            "type": "integer"
          }
        }
      }
    },
    "emot": {
      "type": "object",
      "properties": {
        "confidence": {
          "type": "integer"
        }
      }
    },
    "alts": {
      "type": "array",
      "uniqueItems": true,
      "items": {
        "type": "object",
        "properties": {
          "word": {
            "type": "string"
          },
          "wordGroup": {
            "type": "string"
          },
          "confidence": {
            "type": "integer"
          },
          "viability": {
            "type": "integer"
          }
        }
      }
    }
  },
  "required": ["name", "id"]
}
